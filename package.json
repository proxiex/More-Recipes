{
  "name": "morerecipes",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "jest": {
    "automock": false,
    "verbose": true,
    "setupFiles": [
      "./setup.js"
    ],
    "modulePathIgnorePatterns": [],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/assetsTransformer.js",
      "\\.(css|less)$": "<rootDir>/assetsTransformer.js"
    },
    "collectCoverage": true,
    "collectCoverageFrom":[
       "client/**/*.{js,jsx}"
    ],
    "coveragePathIgnorePatterns": [
      "client/tests",
      "client/coverage",
      "client/utils/firebase.js",
      "client/assets",
      "client/index.jsx",
      "client/rootReducer.js"
    ]
  },
  "scripts": {
    "test": "NODE_ENV=test nyc --reporter=html --reporter=text mocha ./server/tests/*.* --compilers js:babel-register",
    "test:client": "jest ./client ",
    "start": "nodemon ./server/dist/server.js",
    "react": "nodemon --watch server --exec babel-node -- server/src/server.js",
    "hot": "webpack-dev-server --hot --config ./webpack.dev.client.js",
    "build": "babel ./server/src -d ./server/dist",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "coveralls": "nyc --reporter=lcov --reporter=text-lcov npm test",
    "postinstall": "npm run build",
    "client": "jest",
    "start:dev": "nodemon --watch server/src --exec babel-node  -- server/src/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/proxiex/More-Recipes.git"
  },
  "author": "Samuel Longshak",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/proxiex/More-Recipes/issues"
  },
  "homepage": "https://github.com/proxiex/More-Recipes#readme",
  "dependencies": {
    "axios": "^0.17.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
